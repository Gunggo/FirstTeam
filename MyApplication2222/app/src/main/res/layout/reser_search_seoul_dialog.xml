        <CheckBox
            android:id="@+id/yungDeong"
            style="@style/SearchLocal"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:text="영등포구(11)" />

        <CheckBox
            android:id="@+id/yooogSan"
            style="@style/SearchLocal"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:text="용산구(8)" />
    </LinearLayout>

    <LinearLayout
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:orientation="horizontal">

        <CheckBox
            android:id="@+id/enPeyng"
            style="@style/SearchLocal"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:text="은평구(6)" />

        <CheckBox
            android:id="@+id/jongRo"
            style="@style/SearchLocal"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:text="종로구(3)" />

        <CheckBox
            android:id="@+id/joongGugasikiduna"
            style="@style/SearchLocal"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:text="중구(2)" />
    </LinearLayout>

    <LinearLayout
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:orientation="horizontal">

        <CheckBox
            android:id="@+id/joongRang"
            style="@style/SearchLocal"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:text="중랑구(3)" />

    </LinearLayout>

    <Button
        android:id="@+id/button5"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_gravity="center"
        android:layout_marginTop="20dp"
        android:layout_marginBottom="20dp"
        android:background="@drawable/button_radius_check"
        android:text="확인"
        android:textColor="#FFFFFF" />

    <TextView
        android:id="@+id/resetLocal"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_gravity="center"
        android:text="선택 초기화"
        android:textSize="14sp"
        android:textStyle="bold" />

    <ImageButton
        android:id="@+id/closeDialog1"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_gravity="right"
        android:layout_marginRight="20dp"
        android:background="#FFFFFF"
        app:srcCompat="@drawable/side_close_btn" />

</LinearLayout>